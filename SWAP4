#include <stdio.h>
void swap_no(int*,int*);
int main()
{
  int x,y;
  printf("enter the values to be swapped");
  scanf("%d",&x);
  scanf("%d",&y);
  swap_no(&x,&y);
  printf("values after swapping:\n%d\n%d",x,y);
  return 0;
}

void swap_no(int* x,int* y)
{
    int temp;
  temp=(*x);
  (*x)=(*y);
  (*y)=temp;


}
